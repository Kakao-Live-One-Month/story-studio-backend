{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "rewrites": [
      {
        "source": "/api/story/(.*)",
        "destination": "/api/story.js"
      },
      {
        "source": "/api/payment/(.*)",
        "destination": "/api/payment.js"
      },
      {
        "source": "/api/pdf/(.*)",
        "destination": "/api/pdf.js"
      },
      {
        "source": "/api/upload/(.*)",
        "destination": "/api/upload.js"
      }
    ]
  }