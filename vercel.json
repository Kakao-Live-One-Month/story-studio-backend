{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "rewrites": [
      {
        "source": "/api/story/(.*)",
        "destination": "/api/story"
      },
      {
        "source": "/api/payment/(.*)",
        "destination": "/api/payment"
      },
      {
        "source": "/api/pdf/(.*)",
        "destination": "/api/pdf"
      },
      {
        "source": "/api/upload/(.*)",
        "destination": "/api/upload"
      }
    ]
  }